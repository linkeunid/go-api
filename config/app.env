APP_ENV=production
PORT=8080
SERVER_READ_TIMEOUT=10s
SERVER_WRITE_TIMEOUT=10s
SERVER_SHUTDOWN_TIMEOUT=10s
LOG_LEVEL=info
LOG_FORMAT=json
LOG_OUTPUT_PATH=stdout
LOG_FILE_PATH=./tmp/logs/app.log
LOG_FILE_MAX_SIZE=100
LOG_FILE_MAX_BACKUPS=3
LOG_FILE_MAX_AGE=28
LOG_FILE_COMPRESS=true
LOG_ROTATION_TYPE=daily
REDIS_ENABLED=true
REDIS_HOST=redis
REDIS_PORT=6380
REDIS_DB=0
REDIS_CACHE_TTL=15m
REDIS_QUERY_CACHING=true
REDIS_KEY_PREFIX=linkeun_api:
REDIS_POOL_SIZE=10 